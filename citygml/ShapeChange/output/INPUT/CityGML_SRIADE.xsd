  <complexType name="DomainPropertyType">
    <sequence>
      <element ref="sri:Domain"/>
    </sequence>
  </complexType>
  <element name="FunctionalityLevel" substitutionGroup="gml:_Feature" type="sri:FunctionalityLevelType">
    <annotation>
      <documentation>Description of the functionality level of a Smart Readiness Indicator (SRI). Description contains information such as Level Number, textual description and further remarks.</documentation>
    </annotation>
  </element>
  <complexType name="FunctionalityLevelType">
    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence/>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="FunctionalityLevelPropertyType">
    <sequence minOccurs="0">
      <element ref="sri:FunctionalityLevel"/>
    </sequence>
    <attributeGroup ref="gml:AssociationAttributeGroup"/>
  </complexType>
  <element name="Methodology" substitutionGroup="gml:_Feature" type="sri:MethodologyType"/>
  <complexType name="MethodologyType">
    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence>
          <element name="PrefferedWeightings"/>
          <element name="PrefferedServiceCatalogue"/>
          <element name="HeatingPresent"/>
          <element name="CoolingDomainPresent"/>
          <element name="VentilationDomainPresent"/>
          <element name="LightingDomainPresent"/>
          <element name="DynamicBuildingEnvelopeDomainPresent"/>
          <element name="ElectricityDomainPresent"/>
          <element name="ElectricVehicleChargingDomainPresent"/>
          <element name="MonitoringAndControlDomainPresent"/>
          <element maxOccurs="unbounded" minOccurs="0" name="SRIAssesments" type="sri:SRIAssessmentPropertyType"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="MethodologyPropertyType">
