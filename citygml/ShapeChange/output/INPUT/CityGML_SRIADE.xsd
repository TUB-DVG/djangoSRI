    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence>
          <element name="Name"/>
          <element name="Organisation"/>
          <element name="E-Mail"/>
          <element name="PhoneNumber"/>
          <element minOccurs="0" name="Assesor" type="sri:SRIAssessmentPropertyType"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="AssessorPropertyType">
    <sequence minOccurs="0">
      <element ref="sri:Assessor"/>
    </sequence>
    <attributeGroup ref="gml:AssociationAttributeGroup"/>
  </complexType>
  <element name="Building" substitutionGroup="bldg:Building" type="sri:BuildingType">
    <annotation>
      <documentation>ISO 6707-1: construction works that has the provision of shelter for its occupants or contents as one of its main purposes; usually partially or totally enclosed and designed to stand permanently in one place</documentation>
    </annotation>
  </element>
  <complexType name="BuildingType">
    <complexContent>
      <extension base="bldg:BuildingType">
        <sequence/>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="BuildingPropertyType">
    <sequence minOccurs="0">
      <element ref="sri:Building"/>
    </sequence>
    <attributeGroup ref="gml:AssociationAttributeGroup"/>
  </complexType>
  <element name="BuildingUsage" substitutionGroup="gml:_Object" type="sri:BuildingUsageType"/>
  <complexType name="BuildingUsageType">
    <sequence/>
  </complexType>
  <complexType name="BuildingUsagePropertyType">
    <sequence>
      <element ref="sri:BuildingUsage"/>
    </sequence>
  </complexType>
  <element name="ClimateZone" substitutionGroup="gml:_Object" type="sri:ClimateZoneType"/>
  <complexType name="ClimateZoneType">
    <sequence/>
  </complexType>
  <complexType name="ClimateZonePropertyType">
    <sequence>
      <element ref="sri:ClimateZone"/>
    </sequence>
  </complexType>
  <element name="DataCategory" substitutionGroup="gml:_Object" type="sri:DataCategoryType"/>
  <complexType name="DataCategoryType">
    <sequence>
      <element name="EnergyData"/>
      <element name="IndoorEnvironmentalData"/>
      <element name="OutdoorEnvironmentalData"/>
      <element name="SystemOperationalData"/>
      <element name="ControlSettingData"/>
      <element name="OccupantData"/>
      <element name="DesignBasisData"/>
      <element name="AssetData"/>
      <element name="UtilityGridData"/>
      <element name="OnsiteEnergyGenerationData"/>
      <element name="CyberDeviceData"/>
      <element name="Other"/>
    </sequence>
  </complexType>
  <complexType name="DataCategoryPropertyType">
    <sequence>
      <element ref="sri:DataCategory"/>
    </sequence>
  </complexType>
  <element name="DataCategoy" substitutionGroup="gml:_Feature" type="sri:DataCategoyType"/>
  <complexType name="DataCategoyType">
    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence/>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="DataCategoyPropertyType">
    <sequence minOccurs="0">
      <element ref="sri:DataCategoy"/>
    </sequence>
    <attributeGroup ref="gml:AssociationAttributeGroup"/>
  </complexType>
  <element name="Domain" substitutionGroup="gml:_Object" type="sri:DomainType"/>
  <complexType name="DomainType">
    <sequence/>
