    <restriction base="string">
      <enumeration value="surveyed"/>
      <enumeration value="calculated"/>
      <enumeration value="interpolated"/>
      <enumeration value="estimated"/>
      <enumeration value="assumed"/>
      <enumeration value="unknown"/>
    </restriction>
  </simpleType>
  <element name="SRIAssessment" substitutionGroup="gml:_Feature" type="sri:SRIAssessmentType">
    <annotation>
      <documentation>Smart Readiness Indicator (SRI) Assessment of a building. The Assessment is conducted by an Assessor, and leads to a rating of a specific building. Depending on the specifics (such as location, type of building, catalogue considered relevant) different types of services are investigated.</documentation>
    </annotation>
  </element>
  <complexType name="SRIAssessmentType">
    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence>
          <element name="Score" type="integer"/>
          <element name="Date" type="boolean"/>
          <element name="FullBuilding" type="boolean"/>
          <element maxOccurs="unbounded" name="SRI Assesments" type="sri:SRIServicePropertyType"/>
          <element maxOccurs="unbounded" minOccurs="0" name="SRI Assesments" type="sri:AssessorPropertyType"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="SRIAssessmentPropertyType">
    <sequence minOccurs="0">
      <element ref="sri:SRIAssessment"/>
    </sequence>
    <attributeGroup ref="gml:AssociationAttributeGroup"/>
  </complexType>
  <element name="SRIService" substitutionGroup="gml:_Feature" type="sri:SRIServiceType">
    <annotation>
      <documentation>Contains the information on a specific service.</documentation>
    </annotation>
  </element>
  <complexType name="SRIServiceType">
    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence>
          <element name="Domain" type="integer"/>
          <element name="Impact" type="integer"/>
          <element name="Code"/>
          <element name="Name" type="integer"/>
          <element name="Preconditions" type="integer"/>
          <element maxOccurs="unbounded" name="SRI Services" type="sri:SRIAssessmentPropertyType"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="SRIServicePropertyType">
    <sequence minOccurs="0">
      <element ref="sri:SRIService"/>
    </sequence>
    <attributeGroup ref="gml:AssociationAttributeGroup"/>
  </complexType>
